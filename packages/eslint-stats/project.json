{
  "name": "eslint-stats",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/eslint-stats/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project eslint-stats --web",
  "targets": {
    "stats": {
      "dependsOn": ["build"],
      "command": "node {projectRoot}/dist/eslint-stats.js",
      "options": {
        "forwardAllArgs": true
      }
    },
    "measure": {
      "dependsOn": ["build"],
      "command": "node packages/eslint-stats/dist/eslint-stats.js measure",
      "options": {
        "forwardAllArgs": true
      }
    }
  }
}
